<+><project xmlns=\"http://maven.apache.org/POM/4.0.0\"\n         xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\"\n         xsi:schemaLocation=\"http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd\">\n    <modelVersion>4.0.0</modelVersion>\n\n    <!-- one-liner to take you to the cloud with settings form the application.yaml file: -->\n    <!-- 'mvn clean package com.google.cloud.tools:jib-maven-plugin:dockerBuild -Dimage=distroless-hapi && docker run -p 8080:8080 -e spring.batch.job.enabled=false distroless-hapi' -->\n    <!--\n    Note: HAPI projects use the \"hapi-fhir\" POM as their base to provide easy management.\n    You do not need to use this in your own projects, so the \"parent\" tag and it's\n    contents below may be removed\n    if you are using this file as a basis for your own project.\n    -->\n    <parent>\n        <groupId>ca.uhn.hapi.fhir</groupId>\n        <artifactId>hapi-fhir</artifactId>\n        <version>5.2.0</version>\n    </parent>\n\n    <artifactId>hapi-fhir-jpaserver-starter</artifactId>\n\n    <properties>\n        <java.version>8</java.version>\n        <spring_boot_version>2.3.4.RELEASE</spring_boot_version>\n    </properties>\n\n    <prerequisites>\n        <maven>3.6.3</maven>\n    </prerequisites>\n\n    <packaging>war</packaging>\n\n    <name>HAPI FHIR JPA Server - Starter Project</name>\n\n    <repositories>\n        <repository>\n            <id>oss-snapshots</id>\n            <snapshots>\n                <enabled>true</enabled>\n            </snapshots>\n            <url>https://oss.sonatype.org/content/repositories/snapshots/</url>\n        </repository>\n    </repositories>\n\n    <dependencies>\n        <dependency>\n            <groupId>org.eclipse.jetty.websocket</groupId>\n            <artifactId>websocket-api</artifactId>\n            <version>${jetty_version}</version>\n        </dependency>\n        <dependency>\n            <groupId>org.eclipse.jetty.websocket</groupId>\n            <artifactId>websocket-client</artifactId>\n            <version>${jetty_version}</version>\n        </dependency>\n        <dependency>\n            <groupId>mysql</groupId>\n            <artifactId>mysql-connector-java</artifactId>\n            <version>8.0.16</version>\n        </dependency>\n        <!-- https://mvnrepository.com/artifact/org.postgresql/postgresql -->\n        <dependency>\n            <groupId>org.postgresql</groupId>\n            <artifactId>postgresql</artifactId>\n            <version>42.2.9</version>\n        </dependency>\n\n        <!-- Needed for Email subscriptions -->\n        <dependency>\n            <groupId>com.sun.mail</groupId>\n            <artifactId>javax.mail</artifactId>\n            <exclusions>\n                <exclusion>\n                    <groupId>javax.activation</groupId>\n                    <artifactId>activation</artifactId>\n                </exclusion>\n            </exclusions>\n        </dependency>\n\n        <!-- This dependency includes the core HAPI-FHIR classes -->\n        <dependency>\n            <groupId>ca.uhn.hapi.fhir</groupId>\n            <artifactId>hapi-fhir-base</artifactId>\n            <version>${project.version}</version>\n        </dependency>\n\n        <!-- This dependency includes the JPA server itself, which is packaged separately from the rest of HAPI FHIR -->\n        <dependency>\n            <groupId>ca.uhn.hapi.fhir</groupId>\n            <artifactId>hapi-fhir-jpaserver-base</artifactId>\n            <version>${project.version}</version>\n            <exclusions>\n                <exclusion>\n                    <groupId>org.springframework</groupId>\n                    <artifactId>spring-jcl</artifactId>\n                </exclusion>\n                <exclusion>\n                    <groupId>commons-logging</groupId>\n                    <artifactId>commons-logging</artifactId>\n                </exclusion>\n            </exclusions>\n        </dependency>\n        <!-- This dependency includes the JPA EMPI Server -->\n        <dependency>\n            <groupId>ca.uhn.hapi.fhir</groupId>\n            <artifactId>hapi-fhir-jpaserver-empi</artifactId>\n            <version>${project.version}</version>\n        </dependency>\n        <!-- This dependency is used for the \"FHIR Tester\" web app overlay -->\n        <dependency>\n            <groupId>ca.uhn.hapi.fhir</groupId>\n            <artifactId>hapi-fhir-testpage-overlay</artifactId>\n            <version>${project.version}</version>\n            <type>war</type>\n            <scope>provided</scope>\n        </dependency>\n        <dependency>\n            <groupId>ca.uhn.hapi.fhir</groupId>\n            <artifactId>hapi-fhir-testpage-overlay</artifactId>\n            <version>${project.version}</version>\n            <classifier>classes</classifier>\n\n        </dependency>\n\n        <!-- HAPI-FHIR uses Logback for logging support. The logback library is included automatically by Maven as a part of the hapi-fhir-base dependency, but you also need to include a logging library. Logback\n            is used here, but log4j would also be fine. -->\n        <dependency>\n            <groupId>ch.qos.logback</groupId>\n            <artifactId>logback-classic</artifactId>\n        </dependency>\n\n        <!-- Needed for JEE/Servlet support -->\n        <dependency>\n            <groupId>javax.servlet</groupId>\n            <artifactId>javax.servlet-api</artifactId>\n            <scope>provided</scope>\n        </dependency>\n\n        <!-- If you are using HAPI narrative generation, you will need to include Thymeleaf as well. Otherwise the following can be omitted. -->\n        <dependency>\n            <groupId>org.thymeleaf</groupId>\n            <artifactId>thymeleaf</artifactId>\n        </dependency>\n\n        <!-- Used for CORS support -->\n        <!-- Spring Web is used to deploy the server to a web container. -->\n        <dependency>\n            <groupId>org.springframework</groupId>\n            <artifactId>spring-web</artifactId>\n        </dependency>\n\n        <!-- You may not need this if you are deploying to an application server which provides database connection pools itself. -->\n        <dependency>\n            <groupId>org.apache.commons</groupId>\n            <artifactId>commons-dbcp2</artifactId>\n            <exclusions>\n                <exclusion>\n                    <groupId>commons-logging</groupId>\n                    <artifactId>commons-logging</artifactId>\n                </exclusion>\n            </exclusions>\n        </dependency>\n\n        <!-- This example uses H2 embedded database. If you are using another database such as Mysql or Oracle, you may omit the following dependencies and replace them with an appropriate database client\n            dependency for your database platform. -->\n        <dependency>\n            <groupId>com.h2database</groupId>\n            <artifactId>h2</artifactId>\n        </dependency>\n\n        <!-- webjars -->\n        <dependency>\n            <groupId>org.webjars</groupId>\n            <artifactId>bootstrap</artifactId>\n            <version>3.3.7</version>\n        </dependency>\n        <dependency>\n            <groupId>org.webjars</groupId>\n            <artifactId>Eonasdan-bootstrap-datetimepicker</artifactId>\n            <version>4.17.43</version>\n        </dependency>\n        <dependency>\n            <groupId>org.webjars</groupId>\n            <artifactId>font-awesome</artifactId>\n            <version>5.8.2</version>\n        </dependency>\n        <dependency>\n            <groupId>org.webjars.bower</groupId>\n            <artifactId>awesome-bootstrap-checkbox</artifactId>\n            <version>1.0.1</version>\n        </dependency>\n        <dependency>\n            <groupId>org.webjars</groupId>\n            <artifactId>jstimezonedetect</artifactId>\n            <version>1.0.6</version>\n        </dependency>\n        <dependency>\n            <groupId>org.webjars</groupId>\n            <artifactId>select2</artifactId>\n            <version>4.0.3</version>\n        </dependency>\n        <dependency>\n            <groupId>org.webjars.bower</groupId>\n            <artifactId>jquery</artifactId>\n            <version>3.3.1</version>\n        </dependency>\n        <dependency>\n            <groupId>org.webjars.bower</groupId>\n            <artifactId>moment</artifactId>\n            <version>2.15.1</version>\n        </dependency>\n\n        <!-- The following dependencies are only needed for automated unit tests, you do not neccesarily need them to run the example. -->\n        <dependency>\n            <groupId>org.eclipse.jetty</groupId>\n            <artifactId>jetty-servlets</artifactId>\n            <scope>test</scope>\n        </dependency>\n        <dependency>\n            <groupId>org.eclipse.jetty</groupId>\n            <artifactId>jetty-servlet</artifactId>\n            <scope>test</scope>\n        </dependency>\n        <dependency>\n            <groupId>org.eclipse.jetty.websocket</groupId>\n            <artifactId>websocket-server</artifactId>\n            <scope>test</scope>\n        </dependency>\n        <dependency>\n            <groupId>org.eclipse.jetty</groupId>\n            <artifactId>jetty-server</artifactId>\n            <scope>test</scope>\n        </dependency>\n        <dependency>\n            <groupId>org.eclipse.jetty</groupId>\n            <artifactId>jetty-util</artifactId>\n            <scope>test</scope>\n        </dependency>\n        <dependency>\n            <groupId>org.eclipse.jetty</groupId>\n            <artifactId>jetty-webapp</artifactId>\n            <scope>test</scope>\n        </dependency>\n\n\n        <!--\n        For some reason JavaDoc crashed during site generation unless we have this dependency\n        -->\n        <dependency>\n            <groupId>javax.interceptor</groupId>\n            <artifactId>javax.interceptor-api</artifactId>\n            <scope>provided</scope>\n        </dependency>\n\n        <dependency>\n            <groupId>ca.uhn.hapi.fhir</groupId>\n            <artifactId>hapi-fhir-test-utilities</artifactId>\n            <version>${project.version}</version>\n            <scope>test</scope>\n        </dependency>\n\n        <dependency>\n            <groupId>org.awaitility</groupId>\n            <artifactId>awaitility</artifactId>\n            <version>4.0.0-rc1</version>\n            <scope>test</scope>\n        </dependency>\n\n        <dependency>\n            <groupId>org.springframework.boot</groupId>\n            <artifactId>spring-boot-autoconfigure</artifactId>\n            <version>${spring_boot_version}</version>\n        </dependency>\n\n\n        <dependency>\n            <groupId>org.junit.jupiter</groupId>\n            <artifactId>junit-jupiter-api</artifactId>\n            <version>5.6.2</version>\n            <scope>test</scope>\n        </dependency>\n        <dependency>\n            <groupId>org.junit.jupiter</groupId>\n            <artifactId>junit-jupiter-engine</artifactId>\n            <version>5.6.2</version>\n            <scope>test</scope>\n        </dependency>\n\n        <dependency>\n            <groupId>org.springframework.boot</groupId>\n            <artifactId>spring-boot-starter-test</artifactId>\n            <version>${spring_boot_version}</version>\n            <scope>test</scope>\n        </dependency>\n\n\n    </dependencies>\n\n    <build>\n\n        <!-- Tells Maven to name the generated WAR file as ROOT.war -->\n        <finalName>ROOT</finalName>\n\n        <plugins>\n            <plugin>\n                <groupId>org.springframework.boot</groupId>\n                <artifactId>spring-boot-maven-plugin</artifactId>\n                <executions>\n                    <execution>\n                        <goals>\n                            <goal>repackage</goal>\n                        </goals>\n                        <configuration>\n                            <main